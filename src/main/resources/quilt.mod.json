        "id": "jsonite",
        "version": "${version}",
        "metadata": {
            "description": "Adds a system that allows mods to be made with simple JSON files",
            "contributors": {
							"Beez131": "Owner/Developer"
            },
            "contact": {},
            "icon": "assets/jsonite/icon.png",
            "license": "MIT"
        },
        "intermediate_mappings": "net.fabricmc:intermediary",
